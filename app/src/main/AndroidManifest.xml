<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softnopal.myfinalpet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.softnopal.myfinalpet.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.softnopal.myfinalpet.myFavoritePet"
            android:parentActivityName="com.softnopal.myfinalpet.MainActivity">

            <!-- Para versiones 4.0 o menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.softnopal.myfinalpet.MainActivity" />
        </activity>
        <activity android:name="com.softnopal.myfinalpet.contactos" />
        <activity android:name="com.softnopal.myfinalpet.desarrollador"></activity>
    </application>

</manifest>